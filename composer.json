{
  "name": "hassan/one-loop",
  "description": "A Laravel/PHP Package for Minimizing Collection/Array Iterations",
  "keywords": [
    "hassan",
    "one-loop",
    "collect",
    "collection",
    "laravel-collection",
    "performance",
    "optimization"
  ],
  "homepage": "https://github.com/dhassanali/one-loop",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Hassan Ali",
      "email": "hello@hassan-ali.me"
    }
  ],
  "require": {
    "php": "^7.2.5|^8.0|^8.1|^8.2|^8.3",
    "illuminate/support": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
    "illuminate/contracts": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5|^9.0|^10.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Hassan\\OneLoop\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hassan\\OneLoop\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hassan\\OneLoop\\OneLoopServiceProvider"
      ]
    }
  }
}